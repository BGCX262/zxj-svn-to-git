<?xml version="1.0" encoding="ISO-8859-1"?>
<pageflow>
	<name>zXSavedQuery</name>
	<helpid>zXSavedQuery_helpid</helpid>
	<htmldebug></htmldebug>
	<startaction>query</startaction>
	<propagateqs>zYes</propagateqs>
	<version><![CDATA[1.0]]></version>
	<comment><![CDATA[Presents a list of saved search form queries. On clicking one, sets the field in the calling window and closes itself.]]></comment>
	<baseurl>
		<url><![CDATA[../jsp/zXGPF.jsp]]></url>
		<urltype>zFixed</urltype>
	</baseurl>
	<actions>
		<action type="zQuery">
			<helpid>zXSavedQuery_query_helpid</helpid>
			<name>query</name>
			<left>0</left>
			<top>0</top>
			<link>
				<action><![CDATA[listform]]></action>
			</link>
			<formaction>
				<urltype>zAction</urltype>
			</formaction>
			<entities>
				<item>
					<name>zXQry</name>
					<entity><![CDATA[zXQry]]></entity>
					<selectlistgroup>+,nme</selectlistgroup>
					<pkwheregroup><![CDATA[pf,entity,usrPrf]]></pkwheregroup>
					<listgroup><![CDATA[nme]]></listgroup>
					<resolvefk>zNo</resolvefk>
					<allownew>zNo</allownew>
					<attrvalues>
						<item>
							<source><![CDATA[#qs.-qrypf]]></source>
							<destination><![CDATA[pf]]></destination>
						</item>
						<item>
							<source><![CDATA[#qs.-qryentity]]></source>
							<destination><![CDATA[entity]]></destination>
						</item>
						<item>
							<source><![CDATA[#user]]></source>
							<destination><![CDATA[usrPrf]]></destination>
						</item>
					</attrvalues>
				</item>
			</entities>
			<stickyqsaction></stickyqsaction>
			<cached>zNo</cached>
			<resolvefk>zNo</resolvefk>
			<distinct>zNo</distinct>
			<searchform>zNo</searchform>
			<outerjoin>zNo</outerjoin>
			<whereclauseonly>zNo</whereclauseonly>
			<querytype>zAll</querytype>
		</action>
		<action type="zListForm">
			<helpid>zXSavedQuery_listform_helpid</helpid>
			<name>listform</name>
			<entityaction>query</entityaction>
			<left>0</left>
			<top>0</top>
			<link>
			</link>
			<formaction>
				<urltype>zAction</urltype>
			</formaction>
			<stickyqsaction></stickyqsaction>
			<cached>zNo</cached>
			<refs>
				<item>
					<name>zBack</name>
					<startsubmenu>zNo</startsubmenu>
					<url>
						<url><![CDATA[javascript:top.window.close()]]></url>
						<urltype>zFixed</urltype>
						<frameno>notouch</frameno>
					</url>
					<label>
						<EN><![CDATA[Cancel]]></EN>
					</label>
				</item>
			</refs>
			<resolvefk>zNo</resolvefk>
			<width>100%</width>
			<qsentity>-e</qsentity>
			<qspk>-pk</qspk>
			<maxrows>20</maxrows>
			<autocheck>zNo</autocheck>
			<multilist>zNo</multilist>
			<addparitytoclass>zYes</addparitytoclass>
			<url>
				<url><![CDATA[javascript:top.window.close();zXReplaceText('zXSavedQuery','#labelvalue#');]]></url>
				<urltype>zFixed</urltype>
				<frameno>notouch</frameno>
			</url>
			<resorturl>
				<url><![CDATA[listform]]></url>
				<urltype>zAction</urltype>
			</resorturl>
			<pagingurl>
				<url><![CDATA[listform]]></url>
				<urltype>zAction</urltype>
			</pagingurl>
		</action>
	</actions>
</pageflow>
